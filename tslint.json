{
  "extends": "tslint-microsoft-contrib/recommended",
  "rules": {
    "import-name": [ 
      false,
      null,
      {},
      null,
      { "styled-components": "styled" },
      null,
      { "react": "React" },
      null,
      {
        "ignoreExternalModule": true,
        "case": "any-case"
      }
    ],
    "no-implicit-dependencies": [ false, "dev", ["src"] ],
    "no-submodule-imports": [ false, "src" ],
    "no-relative-imports": [ false, "allow-siblings" ]
  }
}